<?xml version="1.0" ?>
<Lems xmlns="http://www.neuroml.org/lems/0.7.3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/lems/0.7.3 https://raw.githubusercontent.com/LEMS/LEMS/development/Schemas/LEMS/LEMS_v0.7.3.xsd">
  <Dimension name="time" t="1"/>
  <Dimension name="au"/>
  <ComponentType name="linear">
    <Parameter dimension="au" name="lam"/>
    <Constant name="lam" value="au"/>
    <Exposure dimension="au" name="x"/>
    <Dynamics>
      <StateVariable dimension="au" exposure="x" name="x"/>
      <TimeDerivative value="lam * x + c" variable="x"/>
    </Dynamics>
  </ComponentType>
</Lems>
